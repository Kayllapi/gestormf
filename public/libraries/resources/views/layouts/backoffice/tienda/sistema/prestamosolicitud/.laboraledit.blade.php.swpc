            $('.tab-laboral-ingreso').css('display','block');
            $('.tab-laboral-gastooperativo').css('display','block');
            $('.tab-laboral-pagodecuota').css('display','block');
            $('.tab-laboral-otroingreso').css('display','block');
            $('.tab-laboral-gastofamiliar').css('display','block');
            $('.tab-laboral-otrogasto').css('display','block');
            $('.tab-laboral-resultado').css('display','block');
            $('#cont-resultado-ingreso').html('<b>INGRESO</b>');
            $('#resultado_total_ingreso').html('0.00');
        }else if(idfuenteingreso==2 && idgiro==1){
            $('.tab-laboral-venta').css('display','block');
            $('.tab-laboral-costodeventa').css('display','block');
            $('.tab-laboral-gastooperativo').css('display','block');
            $('.tab-laboral-pagodecuota').css('display','block');
            $('.tab-laboral-otroingreso').css('display','block');
            $('.tab-laboral-gastofamiliar').css('display','block');
            $('.tab-laboral-otrogasto').css('display','block');
            $('.tab-laboral-resultado').css('display','block');
            $('#cont-resultado-ingreso').html('<b>INGRESO POR VENTAS</b>');
            $('#resultado_total_ingreso').html('0.00');
        }else if((idfuenteingreso==2 && idgiro==2) || (idfuenteingreso==2 && idgiro==3)){
            $('.tab-laboral-servicio').css('display','block');
            $('.tab-laboral-gastooperativo').css('display','block');
            $('.tab-laboral-pagodecuota').css('display','block');
            $('.tab-laboral-otroingreso').css('display','block');
            $('.tab-laboral-gastofamiliar').css('display','block');
            $('.tab-laboral-otrogasto').css('display','block');
            $('.tab-laboral-resultado').css('display','block');
            $('#cont-resultado-ingreso').html('<b>INGRESO POR TRANSPORTE</b>');
            $('#resultado_total_ingreso').html('0.00');
        }
  }
